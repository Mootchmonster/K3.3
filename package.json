{
    "name": "island-rush-v3.3",
    "version": "3.3.0",
    "description": "Island Rush V3 is a Military Strategy Teaching Tool/Game.",
    "main": "./dist/server.js",
    "scripts": {
        "dev": "concurrently \"npm:devBackend\" \"npm:devFrontend\"",
        "lint": "eslint ./src/**/*.ts",
        "start": "cross-env NODE_ENV=production node -r dotenv/config ./dist/server.js",
        "build": "npm run compileBackend && npm run buildClient && npm run moveFrontend",
        "dev:debug": "concurrently \"npm:devBackend:debug\" \"npm:devFrontend\"",
        "devBackend": "nodemon -r dotenv/config ./src/server.ts",
        "devFrontend": "cd ./src/react-client && npm start",
        "buildClient": "cd ./src/react-client && npm run build && cd ../..",
        "moveFrontend": "copyfiles -u 1 ./src/react-client/build/**/*.* ./dist",
        "compileBackend": "tsc -p . && copyfiles -u 3 ./src/server/pages/**/*.html ./dist/server/pages",
        "devBackend:debug": "nodemon -r dotenv/config --inspect ./src/server.ts",
        "typecheckFrontend": "cd ./src/react-client && tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/island-rush/K3.3.git"
    },
    "author": "Spencer Adolph",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/island-rush/K3.3/issues"
    },
    "homepage": "https://github.com/island-rush/K3.3#readme",
    "devDependencies": {
        "@types/connect-azuretables": "^1.0.0",
        "@types/express": "^4.17.2",
        "@types/express-session": "^1.15.16",
        "@types/express-socket.io-session": "^1.3.2",
        "@types/md5": "^2.1.33",
        "@types/mysql2": "github:types/mysql2",
        "@types/node": "^12.12.17",
        "@types/redux": "^3.6.0",
        "@types/socket.io": "^2.1.4",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "concurrently": "^5.0.1",
        "copyfiles": "^2.1.1",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-typescript": "^6.3.1",
        "eslint-plugin-import": "^2.19.1",
        "nodemon": "^2.0.2",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.3"
    },
    "dependencies": {
        "connect-azuretables": "^1.0.20",
        "connect-loki": "^1.1.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "express-socket.io-session": "^1.3.5",
        "md5": "^2.2.1",
        "mysql2": "^2.0.2",
        "redux": "^4.0.4",
        "socket.io": "^2.3.0"
    }
}
